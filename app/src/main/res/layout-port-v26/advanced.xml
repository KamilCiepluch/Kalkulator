            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:autoSizeTextType="uniform"
            android:textColor="@color/white"
            android:text="@string/Button_4"
            app:backgroundTint="@color/purple_700" />

        <Button
            android:id="@+id/Button_1"
            style="@style/Widget.MaterialComponents.FloatingActionButton"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:autoSizeTextType="uniform"
            android:textColor="@color/white"
            android:text="@string/Button_1"
            app:backgroundTint="@color/purple_700" />


        <Button
            android:id="@+id/Button_0"
            style="@style/Widget.MaterialComponents.FloatingActionButton"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:autoSizeTextType="uniform"
            android:textColor="@color/white"
            android:text="@string/Button_0"
            app:backgroundTint="@color/purple_700" />


    </LinearLayout>


    <LinearLayout
        android:id="@+id/second_col"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"

        app:layout_constraintBottom_toTopOf="@+id/row_end"
        app:layout_constraintLeft_toRightOf="@+id/col2_start"
        app:layout_constraintRight_toLeftOf="@+id/col2_end"
        app:layout_constraintTop_toBottomOf="@+id/Buttons_start"
        app:layout_constraintVertical_weight="1">


        <Button
            android:id="@+id/Button_pi"
